TD_tb_or2.check ;
    %load/vec4 v0x55b0e6527710_0;
    %store/vec4 v0x55b0e65405e0_0, 0, 1;
    %load/vec4 v0x55b0e653e6b0_0;
    %store/vec4 v0x55b0e6540680_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x55b0e65407d0_0;
    %load/vec4 v0x55b0e653e770_0;
    %cmp/ne;
    %jmp/0xz  T_0.0, 6;
