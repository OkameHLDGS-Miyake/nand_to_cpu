#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x60ff5b5dd810 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x60ff5b5dd9a0 .scope module, "tb_nand2" "tb_nand2" 3 3;
 .timescale -9 -12;
v0x60ff5b5f2e30_0 .var "a", 0 0;
v0x60ff5b5f2f00_0 .var "b", 0 0;
v0x60ff5b5f2fd0_0 .net "y", 0 0, L_0x60ff5b5f30d0;  1 drivers
S_0x60ff5b5a4490 .scope task, "check" "check" 3 23, 3 23 0, S_0x60ff5b5dd9a0;
 .timescale -9 -12;
v0x60ff5b5a4670_0 .var "aa", 0 0;
v0x60ff5b5f2790_0 .var "bb", 0 0;
v0x60ff5b5f2850_0 .var "expected", 0 0;
TD_tb_nand2.check ;
    %load/vec4 v0x60ff5b5a4670_0;
    %store/vec4 v0x60ff5b5f2e30_0, 0, 1;
    %load/vec4 v0x60ff5b5f2790_0;
    %store/vec4 v0x60ff5b5f2f00_0, 0, 1;
    %delay 1000, 0;
    %load/vec4 v0x60ff5b5f2fd0_0;
    %load/vec4 v0x60ff5b5f2850_0;
    %cmp/ne;
    %jmp/0xz  T_0.0, 6;
    %vpi_call/w 3 29 "$display", "NG: a=%0b b=%0b expected=%0b got=%0b", v0x60ff5b5a4670_0, v0x60ff5b5f2790_0, v0x60ff5b5f2850_0, v0x60ff5b5f2fd0_0 {0 0 0};
    %vpi_call/w 3 30 "$fatal" {0 0 0};
T_0.0 ;
    %end;
S_0x60ff5b5f28f0 .scope module, "dut" "nand2" 3 11, 4 1 0, S_0x60ff5b5dd9a0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x60ff5b5f30d0 .functor NAND 1, v0x60ff5b5f2e30_0, v0x60ff5b5f2f00_0, C4<1>, C4<1>;
v0x60ff5b5f2b40_0 .net "a", 0 0, v0x60ff5b5f2e30_0;  1 drivers
v0x60ff5b5f2c20_0 .net "b", 0 0, v0x60ff5b5f2f00_0;  1 drivers
v0x60ff5b5f2ce0_0 .net "y", 0 0, L_0x60ff5b5f30d0;  alias, 1 drivers
    .scope S_0x60ff5b5dd9a0;
T_1 ;
    %vpi_call/w 3 19 "$dumpfile", "waves/gates/nand2.vcd" {0 0 0};
    %vpi_call/w 3 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x60ff5b5dd9a0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x60ff5b5dd9a0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60ff5b5a4670_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60ff5b5f2790_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60ff5b5f2850_0, 0, 1;
    %fork TD_tb_nand2.check, S_0x60ff5b5a4490;
    %join;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60ff5b5a4670_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60ff5b5f2790_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60ff5b5f2850_0, 0, 1;
    %fork TD_tb_nand2.check, S_0x60ff5b5a4490;
    %join;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60ff5b5a4670_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60ff5b5f2790_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60ff5b5f2850_0, 0, 1;
    %fork TD_tb_nand2.check, S_0x60ff5b5a4490;
    %join;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60ff5b5a4670_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60ff5b5f2790_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60ff5b5f2850_0, 0, 1;
    %fork TD_tb_nand2.check, S_0x60ff5b5a4490;
    %join;
    %vpi_call/w 3 41 "$display", "OK: nand2 passed all tests." {0 0 0};
    %vpi_call/w 3 42 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "tb/gates/tb_nand2.sv";
